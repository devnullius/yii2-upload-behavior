{
    "name": "devnullius/yii2-upload-behavior",
    "description": "Yii2 file/image upload behavior for ActiveRecord",
    "keywords": [
        "file",
        "image",
        "upload",
        "behavior",
        "resize",
        "thumb"
    ],
    "type": "yii2-extension",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4 < 8.0",
        "ext-gd": "*",
        "devnullius/php-tmp-file": "^1.0",
        "ramsey/uuid": "^4.1",
        "symfony/filesystem": "^5.1",
        "webmozart/assert": "^1.9",
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Samoylov",
            "email": "alexey.samoylov@gmail.com",
            "homepage": "http://yiidreamteam.com/team/alexey",
            "role": "Developer"
        },
        {
            "name": "aram",
            "email": "aram.ds@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "devnullius\\upload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "devnullius\\upload\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
